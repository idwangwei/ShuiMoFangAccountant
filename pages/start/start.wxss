/**start.wxss**/
page{
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  font-size: 10pt;
  line-height: 150%;
  min-height: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: stretch;
}
.container{
  width: 100%;
  height: 100%;
  position: absolute;
}
.container-bg{
  width: 100%;
  height: 100%;
}
.content-btn-box{
  position: absolute;
  bottom:10rpx;
  background-color:#fff;
  color:#ff9f4f;
  font-size:36rpx;
  padding:18rpx 130rpx;
  border-radius:13rpx;
  margin:64rpx 0;
  box-shadow:2rpx 5rpx 5rpx #c7c7c7;
  line-height:36rpx;
}
